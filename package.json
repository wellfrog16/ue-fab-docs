{
  "name": "ue-fab-doc",
  "version": ".0.0.1",
  "description": "UE_FAB_DOC",
  "author": "wellfrog",
  "license": "MIT",
  "repository": {
    "type": "*",
    "url": "*"
  },
  "keywords": [
    "ue-fab-doc"
  ],
  "engines": {
    "node": ">=22.17.1"
  },
  "scripts": {
    "commit": "git-cz",
    "docs:build": "vitepress build",
    "docs:deploy-build": "cross-env deploy=gh vitepress build",
    "docs:dev": "vitepress dev",
    "docs:preview": "vitepress preview",
    "lint": "commitlint -e",
    "prepare": "husky install",
    "push": "node node/auto-push.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-git": "^1.3.12",
    "eslint": "^9.32.0",
    "eslint-plugin-format": "^1.0.1",
    "husky": "^8.0.1",
    "vitepress": "^1.6.3",
    "vue": "^3.5.18"
  }
}
